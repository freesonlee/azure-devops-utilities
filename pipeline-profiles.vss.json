{
    "manifestVersion": 1,
    "id": "ads-pipeline-profiles",
    "publisher": "FreesonLi",
    "version": "0.8.0",
    "name": "Pipeline Profiles",
    "description": "Save pipeline resources, parameters and variables",
    "public": false,
    "scopes": [
        "vso.build_execute"
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "pipeline-profiles.overview.md"
        }
    },
    "contributions": [
        {
            "id": "fl.pipeline-profiles.hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-build-web.build-release-hub-group"
            ],
            "properties": {
                "name": "Pipeline Profiles",
                "iconName": "Breakfast",
                "uri": "index.html"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/freesonlee/azure-devops-utilities/src/app/profile-profiles"
    },
    "files": [
        {
            "path": "dist/azure-devops-pipeline-profiles/",
            "addressable": true,
            "packagePath": "/"
        },
        {
            "path": "node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}