{
  "name": "azure-devops-variable-group-editor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-h": "ng build azure-devops-variable-group-editor",
    "build-p": "ng build azure-devops-pipeline-profiles",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pack-history": "npx tfx-cli extension create --manifests ./variable-group-history.vss.json",
    "pack-pipeline": "npx tfx-cli extension create --manifests ./pipeline-profiles.vss.json",
    "publish-history": "npx tfx-cli extension create --manifests ./variable-group-history.vss.json && npx tfx-cli extension publish --manifests ./variable-group-history.vss.json --rev-version --token $ADS_MKT_PAT && rm *.vsix",
    "publish-pipeline": "npx tfx-cli extension create --manifests ./pipeline-profiles.vss.json && npx tfx-cli extension publish --manifests ./pipeline-profiles.vss.json --rev-version --token $ADS_MKT_PAT && rm *.vsix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.1",
    "@angular/cdk": "^17.1.1",
    "@angular/common": "^17.1.1",
    "@angular/compiler": "^17.1.1",
    "@angular/core": "^17.1.1",
    "@angular/forms": "^17.1.1",
    "@angular/material": "^17.1.1",
    "@angular/platform-browser": "^17.1.1",
    "@angular/platform-browser-dynamic": "^17.1.1",
    "@angular/router": "^17.1.1",
    "azure-devops-extension-api": "^2.225.0",
    "azure-devops-extension-sdk": "^3.1.2",
    "js-yaml": "^4.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.1",
    "@angular-devkit/build-angular": "^17.1.1",
    "@angular/cli": "~17.1.1",
    "@angular/compiler-cli": "^17.1.1",
    "@types/jasmine": "~4.3.0",
    "@types/js-yaml": "^4.0.5",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.3"
  }
}