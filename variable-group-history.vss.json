{
    "manifestVersion": 1,
    "id": "ads-var-history",
    "publisher": "FreesonLi",
    "version": "1.0.12",
    "name": "Variable Group History",
    "description": "History support for variable groups",
    "public": true,
    "scopes": [
        "vso.variablegroups_manage",
        "vso.code_write"
    ],
    "icons": {
        "default": "images/variablehistory.png"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "./src/app/variable-group-history/readme.md"
        }
    },
    "contributions": [
        {
            "id": "fl.var-history.hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-build-web.build-release-hub-group"
            ],
            "properties": {
                "name": "Variable Groups",
                "iconName": "Variable",
                "uri": "index.html"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/freesonlee/azure-devops-utilities/tree/vs-extension/src/app/variable-group-history"
    },
    "files": [
        {
            "path": "dist/azure-devops-variable-group-editor/",
            "addressable": true,
            "packagePath": "/"
        },
        {
            "path": "node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}